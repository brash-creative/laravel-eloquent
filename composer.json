{
    "name": "brash-creative/eloquent",
    "description": "Core laravel components for ongoing development work",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Crashley",
            "email": "paulcrashley@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "laravel/framework": "^5.6",
        "brash-creative/laravel-type-collection": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Brash\\Eloquent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brash\\Eloquent\\Tests\\": "tests"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Brash\\Eloquent\\ServiceProvider"
        ]
      }
    }
}
